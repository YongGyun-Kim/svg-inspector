{
  "name": "svg-inspector",
  "version": "2.0.0",
  "description": "Validate the SVG structure to prevent errors",
  "main": "index.js",
  "type": "module",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "style": "npm run lint && npm run format"
  },
  "author": {
    "name": "YongGyun-Kim",
    "email": "kyg7089@naver.com",
    "url": "https://github.com/YongGyun-Kim"
  },
  "license": "MIT",
  "keywords": [
    "svg",
    "validator",
    "inspector",
    "open-source"
  ],
  "bugs": {
    "url": "https://github.com/YongGyun-Kim/svg-inspector/issues"
  },
  "homepage": "https://github.com/YongGyun-Kim/svg-inspector#readme",
  "dependencies": {
    "fast-xml-parser": "^5.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  }
}
